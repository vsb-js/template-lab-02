      "**/?(*.)+(spec|test).[jt]s?(x)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/thehansys/lab-02.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/thehansys/lab-02/issues"
  },
  "homepage": "https://github.com/thehansys/lab-02#readme",
  "devDependencies": {
    "jest": "^28.1.3",
    "prettier": "^2.7.1"
  }
}
